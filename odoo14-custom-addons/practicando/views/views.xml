<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="practicando.list">
      <field name="name">practicando list</field>
      <field name="model">practicando.practicando</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="value"/>
          <field name="value2"/>
        </tree>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="practicando.action_window">
      <field name="name">practicando window</field>
      <field name="res_model">practicando.practicando</field>
      <field name="view_mode">tree,form</field>
    </record>


    <!-- server action to the one above -->
<!--
    <record model="ir.actions.server" id="practicando.action_server">
      <field name="name">practicando server</field>
      <field name="model_id" ref="model_practicando_practicando"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": model._name,
        }
      </field>
    </record>
-->

    <!-- Top menu item -->

    <menuitem name="practicando" id="practicando.menu_root"/>

    <!-- menu categories -->

    <menuitem name="Menu 1" id="practicando.menu_1" parent="practicando.menu_root" action="practicando.action_window"/>
    <menuitem name="Menu 2" id="practicando.menu_2" parent="practicando.menu_root"/>

    <!-- actions -->

    <menuitem name="List" id="practicando.menu_1_list" parent="practicando.menu_2"
              action="practicando.action_window"/>
    <menuitem name="Server to list" id="practicando" parent="practicando.menu_2"
              action="practicando.action_window"/>

  </data>
</odoo>